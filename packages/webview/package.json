{
  "name": "@nzoth/webview",
  "version": "0.1.0",
  "description": "nzoth webview",
  "main": "dist/webview.global.js",
  "module": "dist/webview.esm.js",
  "types": "dist/webview.d.ts",
  "scripts": {
    "dev": "vite build --watch --mode 'development'",
    "build": "vue-tsc --declaration --emitDeclarationOnly && vite build"
  },
  "buildOptions": {
    "name": "NZothWebView",
    "formats": [
      "global"
    ]
  },
  "sideEffects": false,
  "keywords": [
    "nzoth",
    "vue"
  ],
  "dependencies": {
    "@nzoth/bridge": "workspace:^0.1.0",
    "@nzoth/shared": "workspace:^0.1.0",
    "@tweenjs/tween.js": "^18.6.4",
    "@vue/runtime-core": "^3.2.37",
    "@vue/shared": "^3.2.37",
    "intersection-observer": "^0.12.0",
    "lodash.throttle": "^4.1.1",
    "vue": "^3.2.37"
  },
  "devDependencies": {
    "@types/lodash.throttle": "^4.1.7",
    "@vitejs/plugin-vue": "^2.3.3",
    "@vitejs/plugin-vue-jsx": "^1.3.10",
    "@vue/eslint-config-typescript": "^9.1.0",
    "@vue/test-utils": "^2.0.0-rc.21",
    "@vue/vue3-jest": "^27.0.0",
    "autoprefixer": "^10.4.7",
    "eslint-plugin-vue": "^8.7.1",
    "less": "^4.1.2",
    "postcss": "^8.4.13",
    "rollup-plugin-copy": "^3.4.0",
    "vite-plugin-dts": "^1.2.0",
    "vue-tsc": "^0.38.1"
  },
  "browserslist": [
    "iOS >= 11",
    "Android >= 7.0"
  ]
}
